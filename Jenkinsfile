pipeline {
    agent any
    stages {
        stage('Hello world') {
            steps {
                echo 'Hello World it '
            }
        }
        stage('git working'){
            steps{
                echo 'cloning git'
            }
        }
        stage('testing done'){
            steps{
                echo 'testing the project'
            }
        }
        stage('deploying started'){
            steps{
                echo 'trying to deploy'
            }
        }
    }
}
